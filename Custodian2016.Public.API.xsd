<?xml version="1.0" encoding="UTF-8"?>
<!-- edited with XMLSpy v2020 rel. 2 (http://www.altova.com) by  () -->
<xsd:schema xmlns:cust="urn:dmt:custodian2016" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning" targetNamespace="urn:dmt:custodian2016" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.4.4" vc:minVersion="1.1">
	<!--Custodian2016 public API-->
	<xsd:element name="envelope">
		<xsd:complexType>
			<xsd:all>
				<xsd:element name="sourceAPPidentity" type="cust:TGUID"/>
				<xsd:element name="cryptedMessage" type="xsd:string"/>
			</xsd:all>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="request">
		<xsd:annotation>
			<xsd:documentation>Запрос</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="header" type="cust:THeaderRequest"/>
				<xsd:element name="body" type="cust:TbodyRequest"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:element name="responce">
		<xsd:annotation>
			<xsd:documentation>Ответ</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="header" type="cust:THeaderResponce"/>
				<xsd:element name="body" type="cust:TbodyResponce"/>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="THeaderRequest">
		<xsd:annotation>
			<xsd:documentation>Тип данных заголовка запроса</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="RequestType" type="cust:TPacketType">
				<xsd:annotation>
					<xsd:documentation>Тип запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RequestID" type="cust:TGUID">
				<xsd:annotation>
					<xsd:documentation>Идентификатор запроса в формате GUID</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimeStamp" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время формирования запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="sourceAPPidentity" type="cust:TGUID">
				<xsd:annotation>
					<xsd:documentation>Идентификатор приложения сформировавшего запрос
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="replayTo" type="cust:TGUID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор пакета (cust:PacketID) в ответ на который сформировано данное сообщение.
Заполняется исключительно ПО "Хранитель 2016"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndUserID" type="cust:TGUID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор конечного клиентоского приложения/конечного пользователя. Применяется для поддержания принципа FIFO при маштабировании</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TbodyRequest">
		<xsd:annotation>
			<xsd:documentation>Тело запроса</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="customer" type="cust:TCustomer">
				<xsd:annotation>
					<xsd:documentation>RequestType: NewCustomer, UpdateCustomer</xsd:documentation>
					<xsd:documentation>Создание/редактирование карточки клиента/Юрокера/Распорядителя/Управляющего</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="newAccount" type="cust:TnewAccountRequest">
				<xsd:annotation>
					<xsd:documentation>RequestType: NewAccount, UpdateAccount</xsd:documentation>
					<xsd:documentation>Открытие счета/Редактирование анкеты ссуществующего счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SearchCustomer" type="cust:TSearchCustomer">
				<xsd:annotation>
					<xsd:documentation>RequestType: SearcCustomer</xsd:documentation>
					<xsd:documentation>Поиск существующего клиента</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SearchAccount" type="cust:TSearchAccount">
				<xsd:annotation>
					<xsd:documentation>RequestType: SearchAccount</xsd:documentation>
					<xsd:documentation>Поиск существующего открытого счета по реквизитам</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Statement_of_Holdings_Request" type="cust:TStatement_of_Holdings_Request">
				<xsd:annotation>
					<xsd:documentation>RequestType: Statement_of_Holdings</xsd:documentation>
					<xsd:documentation>Запрос выписки пр состояние счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Statement_of_Transactions_Request" type="cust:TStatement_of_Transactions_Request">
				<xsd:annotation>
					<xsd:documentation>RequestType: Statement_of_Transactions</xsd:documentation>
					<xsd:documentation>Запрос выписки по операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Balance" type="cust:TBalance_request">
				<xsd:annotation>
					<xsd:documentation>RequestType: Balance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Transaction" type="cust:TTransaction_request">
				<xsd:annotation>
					<xsd:documentation>RequestType: Transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Transaction_pre_check" type="cust:TTransaction_pre_check_request">
				<xsd:annotation>
					<xsd:documentation>RequestType: RequestType: Transaction_pre_check</xsd:documentation>
					<xsd:documentation>Запрос проверки наличия необходимого количества ЦБ для выполнения транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransactionCommit" type="cust:TTransactionCommit">
				<xsd:annotation>
					<xsd:documentation>RequestType: TransactionCommit</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TransactionRollback" type="cust:TTransactionRollback">
				<xsd:annotation>
					<xsd:documentation>RequestType: TransactionRollback</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="GetDictionary_request" type="cust:TGetDictionary_request">
				<xsd:annotation>
					<xsd:documentation>RequestType: GetDictionary. В качестве значения может выступать или код справочника, в этом случае в ответе будет содержаться только один справочник, или значение '*', в этом случае в ответе будут содержаться все справочники</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="SetDictionary" type="cust:TDictionaryList">
				<xsd:annotation>
					<xsd:documentation>RequestType: SetDictionary.
В теле может содержаться один или несколько справочников</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:complexType name="THeaderResponce">
		<xsd:annotation>
			<xsd:documentation>Тип данных заголовка ответа</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="ResponceType" type="cust:TPacketType">
				<xsd:annotation>
					<xsd:documentation>Тип запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ResponceID" type="cust:TGUID">
				<xsd:annotation>
					<xsd:documentation>Идентификатор запроса в формате GUID</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="TimeStamp" type="xsd:dateTime">
				<xsd:annotation>
					<xsd:documentation>Дата и время формирования запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="RequestID" type="cust:TGUID">
				<xsd:annotation>
					<xsd:documentation>Идентификатор пакета (cust:PacketID) в ответ на который сформировано данное сообщение.
Заполняется исключительно ПО "Хранитель 2016"</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="EndUserID" type="cust:TGUID" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>Идентификатор конечного клиентоского приложения/конечного пользователя. Применяется для поддержания принципа FIFO при маштабировании</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="TbodyResponce">
		<xsd:annotation>
			<xsd:documentation>Тело ответа</xsd:documentation>
		</xsd:annotation>
		<xsd:choice>
			<xsd:element name="customer" type="cust:TCustomer">
				<xsd:annotation>
					<xsd:documentation>Результат запроса на создание новой карточки клиента или редактирование существующей карточки
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="account" type="cust:TAccount">
				<xsd:annotation>
					<xsd:documentation>Результат запроса на открытие счета или редактирование карточки счета
</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="status" type="cust:TStatus">
				<xsd:annotation>
					<xsd:documentation>Статусы обработки запроса</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="error" type="cust:TStatus">
				<xsd:annotation>
					<xsd:documentation>Ошибка при обработке запроса.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Statement_of_Holdings" type="cust:TStatement_of_Holding">
				<xsd:annotation>
					<xsd:documentation>RequestType: Statement_of_Holdings</xsd:documentation>
					<xsd:documentation>Запрос выписки пр состояние счета</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Statement_of_Transactions_Request" type="cust:TStatement_of_Transactions">
				<xsd:annotation>
					<xsd:documentation>RequestType: Statement_of_Transactions</xsd:documentation>
					<xsd:documentation>Запрос выписки по операции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Balance" type="cust:TBalance_responce">
				<xsd:annotation>
					<xsd:documentation>RequestType: Balance</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Transaction" type="cust:TTransaction_responce">
				<xsd:annotation>
					<xsd:documentation>RequestType: Transaction</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="Transaction_pre_check" type="cust:TTransaction_pre_check_responce">
				<xsd:annotation>
					<xsd:documentation>RequestType: RequestType: Transaction</xsd:documentation>
					<xsd:documentation>Запрос проверки наличия необходимого количества ЦБ для выполнения транзакции</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="CustomerList" type="cust:TCustomerList">
				<xsd:annotation>
					<xsd:documentation>RequestType: SearcCustomer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="AccountList" type="cust:TAccountList">
				<xsd:annotation>
					<xsd:documentation>RequestType: SearchAccount</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:choice>
	</xsd:complexType>
	<xsd:simpleType name="TPacketType">
		<xsd:annotation>
			<xsd:documentation>Тип запроса/ответа</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:minLength value="3"/>
			<xsd:maxLength value="100"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TGUID">
		<xsd:annotation>
			<xsd:documentation>Идентификатор запроса или ответа в формате GUID</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:length value="36"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:annotation>
		<xsd:documentation>Справочники представлены в xsd-схеме для ознакомления. Обратите внимание! Финальная версия схемы не будет содержать наполнения справочников</xsd:documentation>
	</xsd:annotation>
	<xsd:simpleType name="Tnssmc_ClientType">
		<xsd:annotation>
			<xsd:documentation>Тип клиента согласно классификации НКЦПФР</xsd:documentation>
			<xsd:documentation>Рішення НКЦПФР № 646 від 08.05.2012</xsd:documentation>
			<xsd:documentation>37. Довідник «Види депонентів, клієнтів депозитарної установи, клієнтів торговця цінними паперами»</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="110">
				<xsd:annotation>
					<xsd:documentation>Фізична особа - резидент</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="120">
				<xsd:annotation>
					<xsd:documentation>Юр. особа - резидент (за винятком нижченаведених кат. юр.осіб</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="128">
				<xsd:annotation>
					<xsd:documentation>Страхові компанії</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="131">
				<xsd:annotation>
					<xsd:documentation>Банки**</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="129">
				<xsd:annotation>
					<xsd:documentation>Торговці цінними паперами</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="310">
				<xsd:annotation>
					<xsd:documentation>Фізична особа - нерезидент</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="320">
				<xsd:annotation>
					<xsd:documentation>Юридична особа - нерезидент</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="121">
				<xsd:annotation>
					<xsd:documentation>Недержавний пенсійний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="122">
				<xsd:annotation>
					<xsd:documentation>Накопичувальний пенсійний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="123">
				<xsd:annotation>
					<xsd:documentation>Інвестиційний фонд*</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="124">
				<xsd:annotation>
					<xsd:documentation>Взаємний фонд інвестиційної компанії*</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="125">
				<xsd:annotation>
					<xsd:documentation>Корпоративний інвестиційний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="126">
				<xsd:annotation>
					<xsd:documentation>Пайовий інвестиційний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="150">
				<xsd:annotation>
					<xsd:documentation>Держава Україна (суб'єкт управління корпоративними правами держави - ФДМУ,інший</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="130">
				<xsd:annotation>
					<xsd:documentation>Держава Україна (суб'єкт управління корпор правами держави - держ господарське</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="140">
				<xsd:annotation>
					<xsd:documentation>Держава Україна (суб'єкт управління корпоративними правами держави - інша особа</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="127">
				<xsd:annotation>
					<xsd:documentation>Фонд банківського управління</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="132">
				<xsd:annotation>
					<xsd:documentation>Нотаріус (депозит)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="133">
				<xsd:annotation>
					<xsd:documentation>Територіальна громада</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Tnssmc_ISIForm">
		<xsd:annotation>
			<xsd:documentation>Справочник НКЦПФР виды ИСИ</xsd:documentation>
			<xsd:documentation>Рішення НКЦПФР № 646 від 08.05.2012</xsd:documentation>
			<xsd:documentation>17. Довідник «Види інституційних інвесторів та накопичувальний пенсійний фонд»</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="01">
				<xsd:annotation>
					<xsd:documentation>інвестиційний фонд*</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="02">
				<xsd:annotation>
					<xsd:documentation>взаємний фонд інвестиційної компанії*</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="03">
				<xsd:annotation>
					<xsd:documentation>пайовий інвестиційний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="04">
				<xsd:annotation>
					<xsd:documentation>корпоративний інвестиційний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="05">
				<xsd:annotation>
					<xsd:documentation>недержавний пенсійний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="06">
				<xsd:annotation>
					<xsd:documentation>накопичувальний пенсійний фонд</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="07">
				<xsd:annotation>
					<xsd:documentation>страхова компанія</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="08">
				<xsd:annotation>
					<xsd:documentation>фонди банківського управління</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="09">
				<xsd:annotation>
					<xsd:documentation>інші фінансові установи</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="Tnssmc_ISIType">
		<xsd:annotation>
			<xsd:documentation>Справочник НКЦПФР типы ИСИ</xsd:documentation>
			<xsd:documentation>Рішення НКЦПФР № 646 від 08.05.2012</xsd:documentation>
			<xsd:documentation>18. Довідник «Типи фондів та інших активів, що перебувають в управлінні компанії з управління активами або торговця цінними паперами - інвестиційного керуючого інвестиційного фонду»</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="01">
				<xsd:annotation>
					<xsd:documentation>відкритий</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="02">
				<xsd:annotation>
					<xsd:documentation>закритий</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="03">
				<xsd:annotation>
					<xsd:documentation>відкритий строковий диверсифікований ІСІ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="04">
				<xsd:annotation>
					<xsd:documentation>відкритий безстроковий диверсифікований ІСІ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="05">
				<xsd:annotation>
					<xsd:documentation>інтервальний строковий диверсифікований ІСІ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="06">
				<xsd:annotation>
					<xsd:documentation>інтервальний безстроковий диверсифікований ІСІ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="07">
				<xsd:annotation>
					<xsd:documentation>закритий строковий диверсифікований ІСІ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="08">
				<xsd:annotation>
					<xsd:documentation>закритий строковий недиверсифікований ІСІ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="09">
				<xsd:annotation>
					<xsd:documentation>закритий строковий недиверсифікований венчурний ІСІ</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="10">
				<xsd:annotation>
					<xsd:documentation>відкритий НПФ </xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="11">
				<xsd:annotation>
					<xsd:documentation>корпоративний НПФ </xsd:documentation>
				</xsd:annotation>
